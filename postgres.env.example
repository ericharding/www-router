# Container configuration for PostgreSQL
CONTAINER_PORT=5432
CONTAINER_READONLY=false
CONTAINER_CAPS=
CONTAINER_VOLUMES=pgdata:/var/lib/postgresql/data:Z
CONTAINER_TMPFS=/tmp,/var/run/postgresql

# PostgreSQL specific environment variables
POSTGRES_DB=myapp
POSTGRES_USER=appuser
POSTGRES_PASSWORD=secure_password_change_me
PGDATA=/var/lib/postgresql/data